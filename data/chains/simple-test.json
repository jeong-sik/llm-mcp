{
  "id": "simple-test",
  "name": "Simple E2E Test Chain",
  "description": "Minimal chain to prove E2E execution works",
  "version": "1.0.0",
  "nodes": [
    {
      "id": "ask-codex",
      "type": "llm",
      "llm": {
        "model": "codex",
        "prompt": "What is 2+2? Answer with just the number."
      },
      "output_key": "codex_answer"
    },
    {
      "id": "ask-gemini",
      "type": "llm",
      "llm": {
        "model": "gemini",
        "prompt": "What is 3+3? Answer with just the number."
      },
      "output_key": "gemini_answer"
    },
    {
      "id": "merge-answers",
      "type": "llm",
      "llm": {
        "model": "ollama",
        "prompt": "Codex said 2+2={{codex_answer}}. Gemini said 3+3={{gemini_answer}}. Sum both results and output just the final number."
      },
      "depends_on": ["ask-codex", "ask-gemini"],
      "output_key": "final_answer"
    }
  ],
  "output": "merge-answers",
  "metadata": {
    "tags": ["test", "e2e"],
    "estimated_duration_sec": 30
  }
}
